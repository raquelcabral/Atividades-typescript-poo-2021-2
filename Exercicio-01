c) NaN

3- QUESTÃO

a)

enum Estados {PI, CE, MA};
 
for (let i= 0; i < 3; i++){
    console.log(Estados[i])
};


b)

enum Estados {PI, CE, MA};
let e  = 0
while (e < 3) {
    
    console.log(e); 
    e = e +1;
};

5- QUESTÃO

function exibir (...letra: string[]) {
    return letra;
}
console.log(exibir('a', 'b'));

 
 
function exibir (...letra: string[]) {
    return letra;
}
console.log(exibir('a', 'b','c'));



function exibir (...letra: string[]) {
    return letra;
}
console.log(exibir('a', 'b','c','d'));



